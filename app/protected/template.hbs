{{nav-bar logout="logout"}}
    <div class="container-fluid">
        <div class="row">
            {{search-box}}
        </div>
        <div class="row">
            {{#jobs-table}}
              {{#each model.jobs as |job|}}
                {{#if job.isActive}}
                  {{job-row location=job.location editedDate=job.editedDate createdBy=job.createdBy}}
                {{/if}}
              {{/each}}
            {{/jobs-table}}
        </div>
    </div>
