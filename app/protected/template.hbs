<html>

<body>

    {{nav-bar logout="logout"}}
    <div class="container-fluid">
        <div class="row">
            {{search-box}}
        </div>
        <div class="row">
            {{#jobs-table}}
              {{#each model.jobs as |job|}}
                <tr>
                    <td>{{job.location}}</td>
                    <td>{{job.editedDate}}</td>
                    <td>{{job.createdBy}}</td>
                </tr>
              {{/each}}
            {{/jobs-table}}
        </div>
    </div>
</body>


</html>
